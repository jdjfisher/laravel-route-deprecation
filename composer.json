{
    "name": "jdjfisher/laravel-route-deprecation",
    "version": "0.1.0",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jdjfisher\\LaravelRouteDeprecation\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Jordan Fisher",
            "email": "jdjfisher@outlook.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
        "laravel/framework": "^8.0|^9.0"
    },
    "require-dev": {
        "phpstan/phpstan": "1.7.x-dev",
        "phpunit/phpunit": "10.0.x-dev"
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Jdjfisher\\LaravelRouteDeprecation\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "check:types": "phpstan analyse --ansi --memory-limit 256M",
        "check:tests": "phpunit --colors=always -d memory_limit=512M",
        "check": [
            "@check:types",
            "@check:tests"
        ]
    }
}
